<?xml version="1.0" encoding="UTF-8"?>
<addon
  id="audioencoder.wav"
  version="20.0.0"
  name="Wav Audio Encoder"
  provider-name="jmarshall">
  <requires>@ADDON_DEPENDS@</requires>
  <extension
    point="kodi.audioencoder"
    extension=".wav"
    library_@PLATFORM@="@LIBRARY_FILENAME@"/>
  <extension point="xbmc.addon.metadata">
    <platform>@PLATFORM@</platform>
    <license>GPL-2.0-or-later</license>
    <source>https://github.com/xbmc/audioencoder.wav</source>
    <assets>
      <icon>icon.png</icon>
    </assets>
    <summary lang="da_DK">Waveform lydfilformat indkoder</summary>
    <summary lang="de_DE">Waveform Audio File Format Encoder</summary>
    <summary lang="en_GB">Waveform Audio File Format Encoder</summary>
    <summary lang="ko_KR">Waveform 오디오 파일 형식 인코더</summary>
    <summary lang="pl_PL">Koder formatu pliku dźwiękowego Waveform</summary>
    <summary lang="ru_RU">Кодировщик аудиофайлов Waveform (WAV)</summary>
    <summary lang="zh_CN">Waveform 音频文件格式编码器</summary>
    <description lang="da_DK">WAV (Waveform Audio Format) er et ukomprimeret lydformat.</description>
    <description lang="de_DE">WAV (Waveform Audio Format) ist ein unkomprimiertes Klangformat.</description>
    <description lang="en_GB">WAV (Waveform Audio Format) is an uncompressed sound format.</description>
    <description lang="ko_KR">WAV(Waveform Audio Format)는 압축되지 않은 사운드 형식입니다.</description>
    <description lang="pl_PL">WAV (Waveform Audio Format) to nieskompresowany format dźwięku.</description>
    <description lang="ru_RU">WAV (Waveform Audio Format) - это формат аудиофайла с несжатым звуковым потоком.</description>
    <description lang="zh_CN">WAV（Waveform 音频格式）是一种未压缩的声音格式。</description>
  </extension>
</addon>
